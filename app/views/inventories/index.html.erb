      <th>Chicken Blu/Grn Egg</th>
      <th>Chicken Bantam Egg</th>
      <th>Duck Egg</th>
      <th>Goose Egg</th>
      <th>Turkey Egg</th>
      <th>Guinea Egg</th>
      <th>Quail Egg</th>


      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventories.each do |inventory| %>
      <tr>
        <td><%= inventory.chickenltbrownlayer %></td>
        <td><%= inventory.chickendrkbrnlayer %></td>
        <td><%= inventory.chickenwhitelayer %></td>
        <td><%= inventory.chickenblugrnlayer %></td>
        <td><%= inventory.chickenbantamlayer %></td>
        <td><%= inventory.duck %></td>
        <td><%= inventory.goose %></td>
        <td><%= inventory.turkey %></td>
        <td><%= inventory.guinea %></td>
        <td><%= inventory.quail %></td>
